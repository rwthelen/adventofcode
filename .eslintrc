{
  "extends": ["eslint:recommended", "google"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "globals": {
    "angular": false
  },
  "rules": {
    "no-console": 0,
    "require-jsdoc": 0,
    "max-len": 0
  }
}
